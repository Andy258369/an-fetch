{
  "name": "an-fetch",
  "version": "2.0.0",
  "description": "一个基于原生fetch封装的强大请求库，具备完整的TypeScript支持，满足企业级应用的各种场景",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "TYPESCRIPT_USAGE.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "webpack --mode=production && npm run build:types",
    "build:dev": "webpack --mode=development && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" \"examples/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" \"examples/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "examples": "node -r ts-node/register examples/complete-usage.ts",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andy258369/an-fetch.git"
  },
  "keywords": [
    "fetch", 
    "http", 
    "client", 
    "ajax", 
    "request", 
    "typescript",
    "axios-like",
    "progress",
    "interceptors",
    "concurrent",
    "transformers",
    "error-handling"
  ],
  "author": "m18680602188@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Andy258369/an-fetch/issues"
  },
  "homepage": "https://github.com/Andy258369/an-fetch#readme",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2",
    "core-js": "^3.35.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}
